int oldLadies = 14;
        int receptionMinetsTime = 10;
        int minetsPerHour = 60;
        int waitingHours = oldLadies * receptionMinetsTime / minetsPerHour;
        int waitingMinets = oldLadies * receptionMinetsTime % minetsPerHour;

        Console.WriteLine($"Время ожидания в очереди {waitingHours} часов и {waitingMinets} минут.");
        Console.ReadKey();
